{
  "name": "ui",
  "version": "0.0.1",
  "private": false,
  "main": "index.ts",
  "scripts": {
    "format": "prettier -w .",
    "check-tsc": "tsc --noEmit",
    "check-eslint": "eslint --cache --fix \"src/**/*.{ts,js,tsx,jsx}\"",
    "lint": "npm run check-tsc && npm run check-eslint",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint-config-common": "./eslint-config-common",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "solid-js": "^1.4.3",
    "svgo": "^2.8.0"
  },
  "pre-commit": [
    "format"
  ],
  "lint-staged": {
    "*.{mjs,jsx,js,ts,tsx}": "npm run lint",
    "*.svg": "svgo"
  },
  "engines": {
    "yarn": "\n\nUse npm please\n\n",
    "pnpm": "\n\nUse npm please\n\n"
  }
}

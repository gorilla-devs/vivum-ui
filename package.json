{
  "name": "ui",
  "version": "0.0.2",
  "private": false,
  "main": "index.ts",
  "scripts": {
    "demo": "vite",
    "format": "prettier -w .",
    "check-tsc": "tsc --noEmit",
    "check-eslint": "eslint --fix \"src/**/*.{ts,js,tsx,jsx}\"",
    "lint": "npm run check-tsc && npm run check-eslint",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "eslint-config-common": "./eslint-config-common",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "sass": "^1.53.0",
    "solid-js": "^1.4.5",
    "svgo": "^2.8.0",
    "vite": "^2.9.13",
    "vite-plugin-solid": "^2.2.6"
  },
  "pre-commit": [
    "format"
  ],
  "lint-staged": {
    "*.{mjs,jsx,js,ts,tsx}": "npm run lint",
    "*.svg": "svgo"
  },
  "engines": {
    "yarn": "\n\nUse npm please\n\n",
    "pnpm": "\n\nUse npm please\n\n"
  },
  "dependencies": {
    "fuzzysort": "^2.0.1"
  }
}
